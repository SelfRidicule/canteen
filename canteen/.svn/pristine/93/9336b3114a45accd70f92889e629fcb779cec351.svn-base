package com.zhongzhou.entity.canteen;

import java.util.Date;

/**
 * 	餐单 反馈
 */
public class MenuFeedback {

	public MenuFeedback() {
		super();
		// TODO Auto-generated constructor stub
	}

	public MenuFeedback(Integer goodState, Date goodTime, Integer menuId, String wxid) {
		super();
		this.goodState = goodState;
		this.goodTime = goodTime;
		this.menuId = menuId;
		this.wxid = wxid;
	}


	/**
	 * 
	 */
	private Integer id;
	
	/**
	 *  1好评 2差评
	 */
	private Integer goodState;
	
	/**
	 *  添加时间或修改时间    只算当天的
	 */
	private Date goodTime;
	
	/**
	 *	餐单id
	 */
	private Integer menuId;
	
	/**
	 *	 
	 */
	private String wxid;

	
	/**
	 * 
	 * @return
	 */
	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	 
	 
	public Integer getGoodState() {
		return goodState;
	}

	public void setGoodState(Integer goodState) {
		this.goodState = goodState;
	}

	public Date getGoodTime() {
		return goodTime;
	}

	public void setGoodTime(Date goodTime) {
		this.goodTime = goodTime;
	}

	public Integer getMenuId() {
		return menuId;
	}

	public void setMenuId(Integer menuId) {
		this.menuId = menuId;
	}

	public String getWxid() {
		return wxid;
	}

	public void setWxid(String wxid) {
		this.wxid = wxid;
	}

	
	 
}
