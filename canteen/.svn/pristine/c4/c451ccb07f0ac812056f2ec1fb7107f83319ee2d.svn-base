		//加载工作日就餐
		function loadGZ(self) {
			$.ajax({
				url: '${Session.BASE_URL}/message/queryMenuMessagePage',
				type: 'POST',
				dataType: 'JSON',
				data: {
					pageNumber: pageNo_3,
					pageSize: pageSize,
					lr: 1
				},
				success: function(result) {
					console.log(result);
					if (result.code = "200") {
						var ul = document.getElementById("gz");
						createFragmentGZ(ul, result.data);

						if (result.data.length > 0) {
							self.endPullUpToRefresh(false);
						} else {
							self.endPullUpToRefresh(true);
						}
					} else {

					}
				}
			});
		}
		//加载工作日就餐
		function createFragmentGZ(ul, response) {
			
			var li = "";
			
			for (var i = 0; i < response.length; i++) {
				
				var name =  response[i].name;
				var time = timestampToTime(response[i].time);
				var content = response[i].content ;
				var avatar =  response[i].avatar ;
				var picurl =  response[i].picurl ;
				var anonymous = response[i].anonymous ;
				
				//1显示 2匿名
				if(anonymous == 1){
					
				}else if(anonymous == 2){
					name = '匿名';
					avatar = "${Session.BASE_URL}/img/menu/nimin_gaitubao_45x41.jpg"
				}else{
					name = '匿名';
					avatar = "${Session.BASE_URL}/img/menu/nimin_gaitubao_45x41.jpg"
				}
				
				var imgLi ;
				
				if(!picurl){
					imgLi =  '';
				}else{
					imgLi = '<div style="width:100%;height:20%;"><img src="${Session.BASE_URL}/'+ picurl +'" style="width:30%;height:30%"> </div>';
				}
				
				 li += '<li class="mui-table-view-cell" style="padding-right:5px;box-shadow:0 5px 5px 0px gray;">'
						+		'<div style="width:100%;height:100%;display:flex;flex-derection:row">'
						+			'<div style="width:20%;height:100%;">'
						+				'<img src="' + avatar +  '" style="width:80%;height:80%"/>'
						+			'</div>'
						+			'<div style="width:80%;height:100%">'
						+				'<div style="width:100%;height:10%;font-size:0.9rem;font-weight:bold">' + name + '</div>'
						+				'<div style="width:100%;height:10%;font-size:0.8rem">' + time + '</div>' 
						+				'<div style="font-size:0.9rem">' + content + ' </div>'
						+				imgLi
						+			'</div>'
						+		'</div>'
						+	'</li>'
						
						
				
					
				
			}

			ul.innerHTML += li;
		}

		//加载节假日套餐
		function loadMF(self) {
			$.ajax({
				url: '${Session.BASE_URL}/message/queryMenuMessagePage',
				type: 'POST',
				dataType: 'JSON',
				data: {
					pageNumber: pageNo_4,
					pageSize: pageSize,
					lr: 2
				},
				success: function(result) {
					console.log(result);
					if (result.code = "200") {
						var ul = document.getElementById("mf");
						createFragmentGZ(ul, result.data);

						if (result.data.length > 0) {
							self.endPullUpToRefresh(false);
						} else {
							self.endPullUpToRefresh(true);
						}
					} else {

					}
				}
			});
		}

		
