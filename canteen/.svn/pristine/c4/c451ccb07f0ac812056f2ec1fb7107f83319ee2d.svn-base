		function CurentTime()
		    { 
		        var now = new Date();
		        
		        var year = now.getFullYear();       //年
		        var month = now.getMonth() + 1;     //月
		        var day = now.getDate();            //日
		        
		        var hh = now.getHours();            //时
		        var mm = now.getMinutes();          //分
		        var ss = now.getSeconds();           //秒
		        
		        var clock = year + "-";
		        
		        if(month < 10)
		            clock += "0";
		        
		        clock += month + "-";
		        
		        if(day < 10)
		            clock += "0";
		            
		        clock += day + " ";
		        
		        if(hh < 10)
		            clock += "0";
		            
		        clock += hh + ":";
		        if (mm < 10) clock += '0'; 
		        clock += mm + ":"; 
		         
		        if (ss < 10) clock += '0'; 
		        clock += ss; 
		        return(clock); 
		}
		
		function timestampToTime(time) {
			var date = new Date(time); //时间戳为10位需*1000，时间戳为13位的话不需乘1000
			Y = date.getFullYear() + '-';
			M = (date.getMonth() + 1 < 10 ? '0' + (date.getMonth() + 1) : date.getMonth() + 1) + '-';
			D = date.getDate() + ' ';
			h = date.getHours() + ':';
			m = date.getMinutes() + ':';
			s = date.getSeconds();
			return Y + M + D;
		}
